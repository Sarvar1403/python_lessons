#1
def user_name():
    name = input("Ismingizni kiriting: ")
    reverse_name = name[::-1]
    random_number = str(random.randint(0, 9))
    username = reverse_name + random_number
    return username

#2
def Convert_add(lst):
    return sum(map(int, lst))
lst = ['1', '3', '5']
result = Convert_add(lst)
print(result)



